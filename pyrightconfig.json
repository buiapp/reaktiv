{
    "include": [
        "src"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        "**/.venv",
        "build",
        "dist"
    ],
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "reportMissingImports": true,
    "reportMissingTypeStubs": true,
    "pythonVersion": "3.9",
    "executionEnvironments": [
        {
            "root": "reaktiv"
        }
    ]
}